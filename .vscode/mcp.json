{
    "servers": {
        "playwright": {
            "command": "npx",
            "args": [
                "@playwright/mcp@latest",
                "--browser=msedge"
            ]
        },
        "github-remote": {
            "type": "http",
            "url": "https://api.githubcopilot.com/mcp/",
        },
        "fetch": {
            "command": "docker",
            "args": ["run", "-i", "--rm", "mcp/fetch"]
        }
    }
}
